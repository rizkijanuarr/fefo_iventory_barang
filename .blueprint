    - database/factories/SupplierFactory.php
    - database/factories/CustomerFactory.php
    - database/factories/BarangFactory.php
    - database/factories/BarangMasukFactory.php
    - database/factories/BarangKeluarFactory.php
    - database/migrations/2024_09_28_075614_create_categories_table.php
    - database/migrations/2024_09_28_075615_create_suppliers_table.php
    - database/migrations/2024_09_28_075616_create_customers_table.php
    - database/migrations/2024_09_28_075617_create_barangs_table.php
    - database/migrations/2024_09_28_075618_create_barang_masuks_table.php
    - database/migrations/2024_09_28_075619_create_barang_keluars_table.php
